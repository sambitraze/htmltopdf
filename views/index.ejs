    <script>
        function printpdf() {
            document.getElementById('info').style.display = 'none';
            window.print();
            document.getElementById('info').style = 'border: 0px solid black;';
        }
        window.onload = async function () {
            var inspection_id = document.getElementById("inspection_id").childNodes[0].nodeValue;
            const response = await fetch('https://api.propdial.co.in/api/inspection/get/' + inspection_id);
            const myJson = await response.json();
            console.log(myJson.data.inspection[0]);
            var date = myJson.data.inspection[0].createdAt;
            var d = new Date(date);
            document.getElementById("visitdate").innerHTML = d.toLocaleString();
        }
    </script>
